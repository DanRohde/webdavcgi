<tr data-unselectable="$unselectable" data-file="$name" data-size="$realsize" data-mime="$mimetype" data-isreadable="$isreadable" data-iswriteable="$iswriteable" data-iseditable="$iseditable" data-type="$type" data-isviewable="$isviewable" data-lastmodified="$lastmodifiedtime" data-displayname="$displayname">
<td class="filename" title="$name$linkinfo <!--IF($config(ALLOW_CHANGEPERM))-->($mode $modestr)<!--ENDIF-->"> 
     <div class="filename">
     <input type="checkbox" name="file" value="$name"/> 
	     <a href="$fileuri" data-action="changeuri">
	     	<img src="$iconurl" class="$iconclass"/>
	     	<span class="nametext">$displayname</span>
	     </a>
     </div>
</td>
<td class="size" title="$sizetitle"> $size </td>
<td class="lastModified" title="$tl(lastmodified): $lastmodified"> $lastmodifiedhr 
</td>
</tr>
