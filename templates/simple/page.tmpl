<!DOCTYPE html>
<html>
<head>
<title>$config(TITLEPREFIX) $uri</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<meta name="application-name" content="WebDAV CGI"/>
<meta name="author" content="(C) ZE CMS, Humboldt-Universitaet zu Berlin, Written by Daniel Rohde"/>
<link rel="alternate" href="?action=mediarss" type="application/rss+xml" title="Media RSS - $uri" id="gallery"/>
$extension(link)
<link rel="stylesheet" type="text/css" href="${VHTDOCS}views/simple/contrib/contrib.min.css"/>
<link rel="stylesheet" type="text/css" href="${VHTDOCS}views/simple/style.min.css"/>
<link rel="stylesheet" type="text/css" href="${VHTDOCS}views/simple/suffix.min.css"/>
<!--IF("$config(CSS)" ne "")--><style>$config(CSS)</style> <!--ENDIF-->
<!--IF("$config(CSSURI)" ne "")--><link rel="stylesheet" type="text/css" href="$config(CSSURI)"/><!--ENDIF-->
$extension(css)
<script src="${VHTDOCS}views/simple/contrib/contrib.min.js"></script>
<script src="${VHTDOCS}views/simple/script.min.js"></script>
$extension(javascript)	
$config(HTMLHEAD)
$INCLUDE(htmlhead)</head>
<body class="collapse-sidebar-listener">
<div id="filler"></div>
<div id="main">
	<div class="langswitch collapse-head-collapsible">$config(LANGSWITCH)</div> 
	<div id="top" class="collapse-head-collapsible">
		$INCLUDE(header)
		$config(HEADER)
	</div>
	<div id="nav" class="collapse-head-listener collapse-sidebar-collapsible">
		<ul id="apps">
			<li title="$tl(files)"><a href="$uri" class="action refresh" title="$tl(files.title)" aria-label="$tl(files.title)" data-action="refresh"><span class="label">$tl(files)</span></a></li>
			$INCLUDE(apps)
			$extension(apps)
		</ul>
		$INCLUDE(nav)
		<ul id="prefs">
			$INCLUDE(pref)
			<!--IF($#SUPPORTED_VIEWS>0)-->
			<li title="$tl(viewoptions)"><div class="accordion">
					<h3>$tl(viewoptions)</h3> 
					<div>
						<ul>$each#@SUPPORTED_VIEWS#'<li title="$tl(${v}view)"><a class="view $v" href="?view=$v" title="$v" aria-label="$v">$tl(${v}view)</a></li>'#'^$view$'#
							<li title="$tl(filter.title)"><a href="#" class="action viewfilter" data-action="viewfilter" data-template="viewfilter" accesskey="o" aria-label="$tl(filter.title)"><span class="label">$tl(filter.title)</span></a></li>
						</ul>
					</div>
				</div>
			</li>
			<!--ELSE-->
				<li title="$tl(filter.title)"><a href="#" class="action viewfilter" data-action="viewfilter" data-template="viewfilter" accesskey="o" aria-label="$tl(filter.title)"><span class="label">$tl(filter.title)</span></a></li>
			<!--ENDIF-->
			<li title="$tl(settings)"><a href="#" data-action="settings" class="dialog action settings" aria-label="$tl(settings)"><span class="label">$tl(settings)</span></a></li>
			$extension(pref)
			$INCLUDE(help)
			<li title="$tl(about)"><a href="#" data-action="about" class="dialog action about" accesskey="8" aria-label="$tl(about)"><span class="label">$tl(about)</span></a></li>
		</ul>
	</div>
<div id="content" class="collapse-head-listener collapse-sidebar-listener">
	<div id="controls" class="collapse-head-listener collapse-sidebar-listener">
		$INCLUDE(toolbar)
	</div>
	<div id="notification"></div>
	<div class="ajax-loader collapse-head-listener collapse-sidebar-listener"></div>
	<div id="flt" data-template="filelisttable" data-uri="$uri" data-baseuri="$baseuri"></div>
	<div id="statusbar">
		<div class="files-stats"><div class="file-icon filecount" title="$tl(statfiles)"></div> <div class="folder-icon dircount" title="$tl(statfolders) "></div> <div class="sum-icon sum" title="$tl(statsum)"></div> <div class="size-icon foldersize" title="$tl(size)"></div></div>
		<div class="selected-files-stats"><input type="checkbox" checked class="unselectable" name="dummy"> <div class="file-icon selfilecount" title="$tl(statfiles)">0</div> <div class="folder-icon seldircount" title="$tl(statfolders)">0</div> <div class="sum-icon selsum" title="$tl(statsum)">0</div> <div class="size-icon selfoldersize" title="0">0</div></div> 
		<div class="notify" title=""></div>
		$extension(statusbar)
	</div>
	$config(FOOTER)
</div>
</div>
<!-- TEMPLATES -->
<div class="template">
	<img id="emptyimage" src="${VHTDOCS}views/simple/icons/blank.png">
	<div id="confirmdialog" data-title="$tl(confirm)"></div>
	<div id="fileactions">
		<ul class="dropdown">
			<li class="dropdown-click"><span class="fileactions-dropdown" title="$tl(fileactions)" tabindex="0"></span><ul class="dropdown-menu">
				<li class="dropdown-menu-item action rename access-writeable" href="#" tabindex="0" data-action="rename" accesskey="m" title="$tl(movefilesbutton)"><span class="label">$tl(movefilesbutton)</span></li>
				$extension(fileaction)
				<li class="dropdown-menu-item action delete" href="#" tabindex="0" data-action="delete" accesskey="d" title="$tl(deletefilesbutton)"><span class="label">$tl(deletefilesbutton)</span></li>
				</ul>
			</li> 
		</ul>
	</div>
	<div id="renamefield">
		<div class="renamefield"> <input type="text" size="50" name="newfilename" value="$filename"/> </div>
	</div>
	<div id="deletefileconfirm">$tl(deletefileconfirm)</div>
	<div id="deletefilesconfirm">$tl(deletefilesconfirm)</div>
	<div id="movefileconfirm">$tl(movefileconfirm)</div>
	<div id="fileuploadconfirm">$tl(fileuploadconfirm)</div>
	<div id="fancybox"></div>
	<div id="paste">$tl(paste)</div>
	<div id="pastecopyconfirm">$tl(pastecopyconfirm)</div>
	<div id="pastecutconfirm">$tl(pastecutconfirm)</div>
	<div id="close">$tl(close)</div>
	<div id="cancel">$tl(cancel)</div>
	<div id="abortdialogtemplate"><div>$tl(pleasewait)</div><div class="action cancel">$tl(cancel)</div></div>
	<div id="confirm">$tl(confirm)</div>
	<div id="tableconfigdialogtemplate">$INCLUDE(tableconfig)</div>
	<div id="titleprefix">$config(TITLEPREFIX)</div>
	$INCLUDE(about)
	<div id="confirmdialogsetting">$tl(confirmdialog.setting)</div>
	<div id="uploadaborted">$tl(upload.aborted)</div>
	$INCLUDE(popupmenu)
	$INCLUDE(settings)
	$extension(templates)
</div>
<!--IF(%AUTOREFRESH)-->
	<div id="autorefreshtimer">
		<span class="autorefreshtimer" title="$tl(autorefresh.select.title)" data-template="$tl(autorefresh.format)"></span> 
		<a href="#" class="autorefreshrunning action autorefreshtoggle" data-action="autorefreshtoggle" title="$tl(autorefresh.toggle)">&nbsp;</a>
		<a href="#" class="autorefreshrunning action autorefreshclear" data-action="autorefreshclear" title="$tl(autorefresh.clear)">&nbsp;</a>
	</div>
<!--ENDIF-->
<!--IF($config(ALLOW_POST_UPLOADS))-->
	<div id="progress" data-title="$tl(upload)"><div class="bar"></div><div class="info"></div></div>
<!--ENDIF-->
<img class="collapsible left action collapse-head" data-action="collapse-head" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglehead)" title="$tl(togglehead)"/>
<img class="collapsible right action collapse-head" data-action="collapse-head" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglehead)" title="$tl(togglehead)"/>
<img class="collapsible top action collapse-sidebar" data-action="collapse-sidebar" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglesidebar)" title="$tl(togglesidebar)"/>
<img class="collapsible bottom action collapse-sidebar" data-action="collapse-sidebar" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglesidebar)" title="$tl(togglesidebar)"/>
$extension(body)
</body>
</html>