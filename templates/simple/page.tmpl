<!DOCTYPE html>
<html>
<head>
<title>$config(TITLEPREFIX) $uri</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
<meta name="application-name" content="WebDAV CGI"/>
<meta name="author" content="(C) ZE CMS, Humboldt-Universitaet zu Berlin, Written by Daniel Rohde"/>
<link rel="alternate" href="?action=mediarss" type="application/rss+xml" title="" id="gallery"/>
<link rel="stylesheet" type="text/css" href="${baseuri}_webdavcgi_/views/simple/contrib/contrib.min.css"/>
<link rel="stylesheet" type="text/css" href="${baseuri}_webdavcgi_/views/simple/style.min.css"/>
<!--IF("$config(CSS)" ne "")--><style>$config(CSS)</style> <!--ENDIF-->
<!--IF("$config(CSSURI)" ne "")--><link rel="stylesheet" type="text/css" href="$config(CSSURI)"/><!--ENDIF-->
$extension(css)
<script src="${baseuri}_webdavcgi_/views/simple/contrib/contrib.min.js"></script>
<script src="${baseuri}_webdavcgi_/views/simple/script.min.js"></script>
$extension(javascript)
$config(HTMLHEAD)
$INCLUDE(htmlhead)</head>
<body>
<div id="filler"></div>
<div id="main">
	<div class="langswitch collapse-head-collapsible">$config(LANGSWITCH)</div> 
	<div id="top" class="collapse-head-collapsible">
		$INCLUDE(header)
		$config(HEADER)
	</div>
	<div id="nav" class="collapse-head-listener collapse-sidebar-collapsible">
		<ul id="apps">
			<li><a href="$uri" class="action refresh" title="$tl(files.title)" data-action="refresh">$tl(files)</a></li>
			$INCLUDE(apps)
			<!--IF($config(ALLOW_SEARCH))-->
			<li><a href="#" class="action search access-readable" data-action="search" data-dialogtemplate="searchdialog" data-resulttemplate="searchresult" accesskey="f">$tl(searchbutton)</a></li>
			<!--ENDIF-->
			<!--IF($config(ALLOW_CHANGEPERM))-->
				<li><a href="#" class="action permissions sel-multi" data-action="permissions" data-template="permissions" accesskey="p">$tl(mode)</a></li>
			<!--ENDIF-->
			<!--IF($config(ENABLE_AFSACLMANAGER))-->
				<li><a href="#" class="action afsaclmanager sel-noneorone sel-dir" data-action="afsaclmanager" data-template="afsaclmanager" accesskey="a">$tl(afs)</a></li>
			<!--ENDIF-->
			<!--IF($config(ENABLE_AFSGROUPMANAGER))-->
				<li><a href="#" class="action afsgroupmanager" data-action="afsgroupmanager" data-template="afsgroupmanager" accesskey="g">$tl(afsgroup)</a></li>
			<!--ENDIF-->
			$extension(apps)
		</ul>
		$INCLUDE(nav)
		<ul id="prefs">
			$INCLUDE(pref)
			<!--IF($#main::SUPPORTED_VIEWS>0)-->
			<li><div class="accordion">
					<h3>$tl(viewoptions)</h3> 
					<div>
						<ul>$each#@main::SUPPORTED_VIEWS#'<li><a class="view $v" href="?view=$v" title="$v">$tl(${v}view)</a></li>'#'$view'#
							<li><a href="#" class="action viewfilter" data-action="viewfilter" data-template="viewfilter" accesskey="o">$tl(filter.title)</a></li>
						</ul>
					</div>
				</div>
			</li>
			<!--ENDIF-->
			<!--IF($#main::SUPPORTED_VIEWS<1)-->
				<li><a href="#" class="action viewfilter" data-action="viewfilter" data-template="viewfilter" accesskey="o">$tl(filter.title)</a></li>
			<!--ENDIF-->
			<li><a href="#" data-action="settings" class="dialog action settings">$tl(settings)</a></li>
			$INCLUDE(help)
			<li><a href="#" data-action="about" class="dialog action about" accesskey="8">$tl(about)</a></li>
		</ul>
	</div>
<div id="content" class="collapse-head-listener collapse-sidebar-listener">
	<div id="controls" class="collapse-head-listener collapse-sidebar-listener">
		<div id="new">
			<a href="#" class="action new" data-action="new" accesskey="n">$tl(new)</a> 
			<!--IF($config(ALLOW_POST_UPLOADS))-->
				<a href="#" class="action upload uibutton access-writeable hideit" id="fileuploadbutton" title="$tl(upload) (&le; $maxuploadsizehr)" accesskey="u">$tl(fileuploadbutton)</a><form id="file-upload-form" enctype="multipart/form-data" method="post" action="#" data-maxfilesize="$maxuploadsize" data-maxfilesizehr="$maxuploadsizehr"><input type="hidden" name="filesubmit" value="yes"/><input class="file-upload" type="file" name="file_upload" multiple="multiple"/></form>
			<!--ENDIF-->
			<!--IF($config(ENABLE_CLIPBOARD))-->			
				<a class="listaction paste hideit sel-none uibutton" href="#" data-action="paste" title="$tl(paste)" accesskey="v">&nbsp;</a>
			<!--ENDIF-->			
			<ul class="new popup hidden">
			<li><a href="#" class="action create-folder access-writeable" data-action="create-folder" accesskey="r">$tl(createfolderbutton)</a></li>
			<li><a href="#" class="action create-file access-writeable" data-action="create-file" accesskey="e">$tl(createnewfilebutton)</a></li>
			<!--IF($config(ALLOW_SYMLINK))-->
				<li><a href="#" class="action create-symlink sel-one access-writeable" data-action="create-symlink"  title="$tl(createsymlinkdescr)" accesskey="h">$tl(createsymlinkbutton)</a></li>
			<!--ENDIF-->
			$extension(new)
			</ul>
		</div>
		<div id="path" class="sel-none hideit">
			<span id="quicknav">$[quicknavpath]</span>
			<form id="pathinputform" action="#" method="post"><span id="pathinput" class="hidden"><input type="text" size="50" name="uri" value="$uri"/><a href="#" data-action="chdir">$tl(go)</a></span></form>
			<a href="#" class="action changedir" data-action="changedir" title="$tl(changedir)">&nbsp;</a>
			<!--IF($config(ENABLE_BOOKMARKS))-->
				<div id="bookmarks">
					<a href="#" class="action addbookmark" data-action="addbookmark" title="$tl(addbookmarktitle)">&nbsp;</a>
					<div id="bookmarksmenu">
					<a href="#" class="action bookmarkmenu" data-action="bookmarkmenu" title="$tl(bookmarks)">&nbsp;</a>
					<ul id="bookmarksmenulist" class="hidden">
						<li id="bookmarktemplate" class="hidden">$bookmarktext<img class="action rmsinglebookmark right neverdisabled" alt="$tl(rmbookmarktitle)" data-action="rmsinglebookmark" data-bookmark="$bookmarkpath" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII="/></li>
						<li class="sep link action bookmarksortpath" data-action="bookmarksortpath">$tl(sortbookmarkbypath)<span class="path right">&uarr;</span><span class="path-desc right">&darr;</span></li>
						<li class="link action bookmarksorttime" data-action="bookmarksorttime">$tl(sortbookmarkbytime)<span class="time right">&uarr;</span><span class="time-desc right">&darr;</span></li>
						<li class="sep link action rmallbookmarks" data-action="rmallbookmarks" title="$tl(rmallbookmarkstitle)">$tl(rmallbookmarks)</li>
					</ul>
					</div>
				</div>
			<!--ENDIF-->
			<!--IF(%main::AUTOREFRESH)-->
				<div id="autorefresh" title="$tl(autorefresh.select.title)">
					<a href="#" class="action autorefreshmenu" data-action="autorefreshmenu">&nbsp;</a>
					<ul id="autorefreshmenulist" class="hidden">
						<li><a href="#" class="action setautorefresh" data-action="setautorefresh" data-value="now">$tl(autorefresh.now)</a></li>
						$each@%main::AUTOREFRESH@'<li><a href="#" class="action setautorefresh" data-action="setautorefresh" data-value="$k">$v</a></li>'@
						<li class="sep"><a href="#" class="autorefreshrunning action autorefreshtoggle" data-action="autorefreshtoggle">$tl(autorefresh.toggle)</a></li>
						<li><a href="#" class="autorefreshrunning action autorefreshclear" data-action="autorefreshclear">$tl(autorefresh.clear)</a></li>
						<li class="sep"><span class="action autorefreshtimer" data-action="autorefreshtimer" data-template="$tl(autorefresh.format)"></span></li> 
					</ul>
				</div>
			<!--ENDIF-->
		</div>
		<div id="filelistactions" class="sel-multi hideit">
			<span id="selstats">$tl(statfiles) $filecount, $tl(statfolders) $dircount $tl(statsum) $sum</span>
			<!--IF($config(ENABLE_CLIPBOARD))-->			
				<a class="listaction copy sel-multi uibutton" href="#" title="$tl(copytooltip)" data-action="copy" accesskey="c">&nbsp;</a>
				<a class="listaction cut sel-multi access-writeable uibutton" href="#" title="$tl(cuttooltip)" data-action="cut" accesskey="x">&nbsp;</a>
			<!--ENDIF-->		
			$extension(filelistaction)	
			<a class="listaction delete sel-multi uibutton" href="#" title="$tl(deletefilestext)" data-action="delete" >$tl(deletefilesbutton)</a>
		</div>
		<!--IF($config(ENABLE_NAMEFILTER))-->
		<div class="filterbox">
			<form id="filterbox" method="post" action="#">
			$tl(namefilter) <input type="text" name="query" size="10" title="$tl(namefiltertooltip)" accesskey="i"/>
			<img class="action clearfilter" data-action="clearfilter" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="-" title="$tl(filter.reset)"/>
			</form>
		</div>
		<!--ENDIF-->
	</div>
	<div id="notification"></div>
	<div class="ajax-loader collapse-head-listener collapse-sidebar-listener"></div>
	<div id="flt" data-template="filelisttable" data-uri="$uri" data-baseuri="$baseuri"></div>
	$config(FOOTER)
</div>
</div>
<!-- TEMPLATES -->
<div class="template">
	<div id="confirmdialog" data-title="$tl(confirm)"></div>
	<div id="fileactions">
	     <span class="fileactions">
		 <a class="action edit access-readable access-writeable" href="#" data-action="edit">$tl(editbutton)</a>
		 <a class="action rename access-writeable" href="#" data-action="rename" accesskey="m">$tl(movefilesbutton)</a>
		 $extension(fileaction)
		 <a class="action delete" href="#" data-action="delete" accesskey="d">$tl(deletefilesbutton)</a>
	     </span>
	</div>
	<div id="renamefield">
		<div class="renamefield"> <input type="text" size="50" name="newfilename" value="$filename"/> </div>
	</div>
	<div id="deletefileconfirm">$tl(deletefileconfirm)</div>
	<div id="deletefilesconfirm">$tl(deletefilesconfirm)</div>
	<div id="movefileconfirm">$tl(movefileconfirm)</div>
	<div id="fileuploadconfirm">$tl(fileuploadconfirm)</div>
	<div id="edittextdata"><form id="edittextdataform" method="post" action="#"><textarea name="textdata" rows="15" cols="70"></textarea><div><a href="#" class="action cancel-edit" data-action="cancel-edit">$tl(cancel)</a><a href="#" class="action savetextdata" data-action="savetextdata">$tl(savebutton)</a></div></form></div>
	<div id="confirmsavetextdata">$tl(confirmsavetextdata)</div>
	<div id="canceledit">$tl(canceledit)</div>
	<div id="fancybox"></div>
	<div id="paste">$tl(paste)</div>
	<div id="pastecopyconfirm">$tl(pastecopyconfirm)</div>
	<div id="pastecutconfirm">$tl(pastecutconfirm)</div>
	<div id="close">$tl(close)</div>
	<div id="cancel">$tl(cancel)</div>
	<div id="confirm">$tl(confirm)</div>
	<div id="tableconfigdialogtemplate">$INCLUDE(tableconfig)</div>
	<div id="titleprefix">$config(TITLEPREFIX)</div>
	<div id="about">
		<span class="nowrap">&copy; 2014 ZE Computer and Media Services, Humboldt-University at Berlin</span><br/>
		<span class="nowrap">Written 2010-2014 by Daniel Rohde &lt;<a href="mailto:d.rohde@cms.hu-berlin.de">d.rohde@cms.hu-berlin.de</a>&gt;</span><br/>
		Homepage: <a href="http://webdavcgi.sf.net/">webdavcgi.sf.net</a>
		<hr/>
		Licensing Information <br/>
		<a href="http://webdavcgi.sf.net">WebDAV CGI</a><br/>
		<a href="http://gplv3.fsf.org/">GPLv3</a> <br/> &nbsp;<br/>
		jQuery, jQuery-UI <br/>
		Plugins: File Upload, Multidraggable,  Noty, Cookie <br/>
		<a href="https://jquery.org/license">MIT License</a><br/>&nbsp;<br/>
		Fancybox<br/>
		<a href="http://creativecommons.org/licenses/by-nc/3.0/">Creative Commons Attribution-NonCommercial 3.0</a>
	</div>
	<div id="confirmdialogsetting">$tl(confirmdialog.setting)</div>
	<div id="uploadaborted">$tl(upload.aborted)</div>
	$INCLUDE(popupmenu)
	$INCLUDE(settings)
	$extension(templates)
</div>
<!--IF(%main::AUTOREFRESH)-->
	<div id="autorefreshtimer" title="$tl(autorefresh.select.title)">
		<span class="action autorefreshtimer" data-action="autorefreshtimer" data-template="$tl(autorefresh.format)"></span> 
		<a href="#" class="autorefreshrunning action autorefreshtoggle" data-action="autorefreshtoggle" title="$tl(autorefresh.toggle)">&nbsp;</a>
		<a href="#" class="autorefreshrunning action autorefreshclear" data-action="autorefreshclear" title="$tl(autorefresh.clear)">&nbsp;</a>
	</div>
<!--ENDIF-->
<!--IF($config(ALLOW_POST_UPLOADS))-->
	<div id="progress" data-title="$tl(upload)"><div class="bar"></div><div class="info"></div></div>
<!--ENDIF-->
<img class="collapsible left action collapse-head" data-action="collapse-head" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglehead)" title="$tl(togglehead)"/>
<img class="collapsible right action collapse-head" data-action="collapse-head" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglehead)" title="$tl(togglehead)"/>
<img class="collapsible top action collapse-sidebar" data-action="collapse-sidebar" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglesidebar)" title="$tl(togglesidebar)"/>
<img class="collapsible bottom action collapse-sidebar" data-action="collapse-sidebar" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" alt="$tl(togglesidebar)" title="$tl(togglesidebar)"/>
$extension(body)
</body>
</html>