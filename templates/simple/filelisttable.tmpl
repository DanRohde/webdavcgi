<div id="fileListTableContent">
	<form id="filelistform" method="post" action="#">
	<input id="filelistaction" type="hidden" name="__set__" value=""/>
	<!--IF($config(SHOW_QUOTA) && parseByteSize("$quotalimit") > 0)-->
	<div id="quota">
			<div class="quotabar" title="$tl(quotaavailable) $quotaavailable (${quotaavailableperc}%) / $quotalimit"><div title="$tl(quotaused) $quotaused (${quotausedperc}%) / $quotalimit" class="$quotalevel quotaused" style="$quotastyle; width: ${quotausedperc}%;">${quotausedperc}%</div></div>
			$tl(quotalimit) <span title="$quotalimittitle">$quotalimit</span>, 
			$tl(quotaused) <span title="$quotausedtitle">$quotaused (${quotausedperc}%)</span>, 
			$tl(quotaavailable) <span title="$quotaavailabletitle">$quotaavailable (${quotaavailableperc}%)</span>
		</div>
	<!--ENDIF-->
	<!--IF($isviewfiltered())--><div class="viewfilterinfo">$tl(filter) $filterInfo() </div><!--ENDIF-->

	<table id="fileListTable" class="isreadable-$isreadable iswriteable-$iswriteable unselectable-$unselectable">
		<thead>
		<tr>
<!--TEMPLATE(selector)[
			<th id="headerSelector" class="sorter-false resizable-false dragaccept -hidden" data-name="selector">
				<ul class="dropdown">
				<li class="dropdown-hover">
					<img src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII=" class="toggleselection" alt="OX" title="$tl(togglealltooltip)"/>&nbsp;
					<ul class="dropdown-menu">
						<li>
							<img alt="X" title="$tl(selectall)" class="selectall" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII="/>
						</li>
						<li>
							<img alt="O" title="$tl(selectnone)" class="selectnone" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAgY0hSTQAAeiYAAICEAAD6AAAAgOgAAHUwAADqYAAAOpgAABdwnLpRPAAAAAJiS0dEAACqjSMyAAAACXBIWXMAAABIAAAASABGyWs+AAAAC0lEQVQI12NgYAAAAAMAASDVlMcAAAAASUVORK5CYII="/>
						</li>
					</ul>
				</li>
				</ul>				
			</th>
]-->
			$filelistheadcolumns
			<th id="headerOption" class="dropaccept resizable-false sorter-false"><div class="tableconfigbutton" title="$tl(tableconfig.button.title)" data-template="tableconfig"></div></th>
			
<!--TEMPLATE(name)[
			<th id="headerName" class="dragaccept resizable-false -hidden" data-name="name" data-sort="data-displayname" data-title="$tl(statfiles) $filecount, $tl(statfolders) $dircount, $tl(statsum) $sum">
				$tl(name)
			</th>
]-->
<!--TEMPLATE(size)[
			<th id="headerSize" data-name="size" data-sort="data-size" data-sorttype="number" data-title="$foldersize" class="dragaccept -hidden">$tl(size)</th>
]-->
<!--TEMPLATE(lastmodified)[
			<th id="headerDate" data-name="lastmodified" data-sort="data-lastmodified" data-sorttype="number" class="dragaccept -hidden">$tl(lastmodified)</th>
]-->
<!--TEMPLATE(mime)[
			<th id="headerMime" data-name="mime" data-sort="data-mime" class="dragaccept -hidden">$tl(mime)</th>
]-->
<!--TEMPLATE(created)[
			<th id="headerCreated" data-name="created" data-sort="data-created" data-sorttype="number" class="dragaccept -hidden">$tl(created)</th>
]-->
<!--TEMPLATE(mode)[
			<th id="headerMode" data-name="mode" class="dragaccept sorter-false -hidden">$tl(mode)</th>
]-->
<!--TEMPLATE(uid)[
			<th id="headerUID" data-name="uid" data-sort="data-uid" class="dragaccept -hidden">UID</th>
]-->
<!--TEMPLATE(gid)[
			<th id="headerGID" data-name="gid" data-sort="data-gid" class="dragaccept -hidden">GID</th>
]-->
		</tr>
		
		</thead>
		<tbody id="fileList" data-entrytemplate="filelistentry" data-uri="$uri" data-thumbnail_pdfps="$config(ENABLE_THUMBNAIL_PDFPS)">
		$filelist(filelistentry)
		</tbody>
	</table>
	</form>
</div>