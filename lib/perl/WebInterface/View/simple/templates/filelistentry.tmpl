<tr data-unselectable="$unselectable" data-file="$name" data-size="$realsize" data-mime="$mimetype" data-isreadable="$isreadable" data-iswriteable="$iswriteable" data-iseditable="$iseditable" data-type="$type" data-isviewable="$isviewable" data-lastmodified="$lastmodifiedtime">
<td class="filename" title="$name$linkinfo"> 
     <div class="filename">
	     <a href="$fileuri"><img src="$iconurl" class="$iconclass"/></a>
	     <input type="checkbox" name="file" value="$name"/> 
	     <a class="nametext" href="$fileuri">$name</a>
     </div>
</td>
<td class="size" title="$sizetitle"> $size </td>
<td class="lastModified" title="$tl(created): $created"> $lastmodified 
</td>
</tr>
