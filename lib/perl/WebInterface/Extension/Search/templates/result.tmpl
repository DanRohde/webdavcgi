<div class="search result entry $type" >
	<div data-href="$fileuri" class="search result entry-info $type" >
		<img src="$iconurl" class="search $iconclass" alt="icon" title="$mime"/>
		<div class="search result entry label" data-htmltooltip="$filename%0D$tl(search.parentfolder): $dirname/%0D$tl(size): $size%0D$tl(lastmodified): $lastmodified%0D$parentfolder">$qfilename</div>
	</div>
	<!--IF( q@$filename@ ne ".")-->
		<div class="search result entry parent folder icon category-folder suffix-folder" data-htmltooltip="$tl(search.openparentfolder)%0D$dirname" data-href="$dirname"></div>
	<!--ELSE-->
		<div class="search result entry dummyparent"></div>
	<!--ENDIF-->
	
</div>